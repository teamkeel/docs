      "patterns": [
        {
          "include": "#double_comment"
        },
        {
          "include": "#attribute"
        },
        {
          "include": "#api_models_definition"
        }
      ],
      "end": "\\s*\\}",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.tag.keel"
        }
      }
    },
    "api_models_definition": {
      "begin": "^\\s*(models)\\s+({)",
      "beginCaptures": {
        "1": {
          "name": "keyword.api.models.keel"
        },
        "2": {
          "name": "punctuation.definition.tag.keel"
        }
      },
      "patterns": [
        {
          "include": "#double_comment"
        },
        {
          "match": "\\s+([A-Za-z][\\w]*)\\s*\\n",
          "captures": {
            "1": {
              "name": "entity.name.type.model.api.keel"
            }
          }
        }
      ],
      "end": "\\s*\\}",
      "endCaptures": {
        "0": {
