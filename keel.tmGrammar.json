        "2": {
          "name": "punctuation.definition.tag.keel"
        }
      },
      "patterns": [
        {
          "include": "#double_comment"
        },
        {
          "begin": "\\s?({)",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.tag.keel"
            }
          },
          "patterns": [
            {
              "include": "#double_comment"
            },
            {
              "include": "#operation_definition"
            }
          ],
          "end": "\\s?(})",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.tag.keel"
            }
          }
        }
      ],
      "end": "(\\n)"
    },
    "api_block_definition": {
      "begin": "\\s*(api)\\s+([A-Za-z][\\w]*)\\s*({)",
      "beginCaptures": {
        "1": {
          "name": "keyword.api.keel"
        },
        "2": {
          "name": "entity.name.type.api.keel"
        },
        "3": {
          "name": "punctuation.definition.tag.keel"
